{
  "editor.renderWhitespace": "boundary",
  "editor.rulers": [100],
  "editor.cursorBlinking": "expand",
  "editor.fontSize": 14,
  "editor.tabSize": 2,
  "window.zoomLevel": 0,
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "workbench.colorTheme": "Night Owl",
  "editor.multiCursorModifier": "alt",
  "editor.formatOnPaste": false,
  "javascript.validate.enable": false,
  "editor.minimap.enabled": false,
  "explorer.confirmDragAndDrop": false,
  "workbench.startupEditor": "newUntitledFile",
  "sync.removeExtensions": true,
  "sync.syncExtensions": true,
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.fontFamily": "monospace",
  "terminal.integrated.shell.osx": "zsh",
  "zenMode.centerLayout": false,
  "zenMode.hideTabs": false,
  "zenMode.fullScreen": false,
  "cSpell.enabledLanguageIds": ["markdown"],
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.formatOnSave": false,
  "workbench.iconTheme": "material-icon-theme",
  "prettier.singleQuote": true,
  "eslint.options": {
    "env": {
      "es6": true
    },
    "parserOption": {
      "ecmaVersion": 7
    }
  },
  "jshint.options": {
    "esversion": 6
  },

  "workbench.activityBar.visible": true,
  "workbench.statusBar.visible": true,
  // "window.zoomLevel": 0.5,
  "workbench.sideBar.location": "left",
  "editor.renderIndentGuides": false,
  "breadcrumbs.enabled": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "docthis.authorName": "sushant suwal",
  "docthis.includeAuthorTag": true,
  // "editor.fontLigatures": true,
  "markdown.preview.fontFamily": "cursive, -apple-system, BlinkMacSystemFont, 'Segoe WPC', 'Segoe UI', 'Ubuntu', 'Droid Sans', sans-serif",
  "editor.snippetSuggestions": "top",
  "workbench.tree.indent": 15,
  "breadcrumbs.filePath": "off",
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.editor.openPositioning": "left",
  "workbench.editor.restoreViewState": false,
  // "workbench.editor.tabCloseButton": "off",
  "workbench.editor.tabSizing": "shrink",
  "files.autoSave": "onFocusChange",
  "files.autoSaveDelay": 0,
  "gitlens.views.fileHistory.location": "gitlens",
  "gitlens.views.lineHistory.location": "gitlens",
  "gitlens.views.compare.location": "gitlens",
  "gitlens.views.search.location": "gitlens",
  "prettier.jsxSingleQuote": true,

  "debug.node.autoAttach": "off",
  "editor.suggestSelection": "first",
  "jshint.enable": false,
  "cSpell.userWords": [
    "Ebanking",
    "Ionicons",
    "Khalti",
    "Pharma",
    "cartitem",
    "middlewares",
    "persistor",
    "recieved",
    "upachar",
    "xlarge"
  ],
  "files.exclude": {
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },
  "todo-tree.tree.showScanModeButton": false,
  "kite.showWelcomeNotificationOnStartup": false,
  "diffEditor.renderSideBySide": false,
  "editor.renameOnType": true,
  "java.semanticHighlighting.enabled": true,
  "diffEditor.ignoreTrimWhitespace": false,
  "workbench.colorCustomizations": {
    "editorUnnecessaryCode.border": "#dd7aab"
  },
  "editor.showDeprecated": true,
  "editor.showUnused": true,
  "yaml.schemas": {
    "file:///Users/sushantsuwal/.vscode/extensions/atlassian.atlascode-2.7.0/resources/schemas/pipelines-schema.json": "bitbucket-pipelines.yml"
  },
  "editor.formatOnType": false
}
